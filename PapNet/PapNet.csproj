<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<AssemblyName>PapNet</AssemblyName>
		<PackageId>PapNet</PackageId>
		<Title>PapNet - Post Affiliate Pro API Helper</Title>
		<PackageDescription>
			Post Affiliate Pro API helper project for quick requests.
		</PackageDescription>
		<Authors>Metin Ozturk</Authors>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<RepositoryUrl>https://github.com/mtnoztrk/papnet</RepositoryUrl>
		<PackageIcon>icon.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>api http post affiliate pro pap</PackageTags>

		<Version>0.0.2</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="$(MSBuildThisFileDirectory)/icon.png" Pack="true" PackagePath="/" />
		<None Include="$(MSBuildThisFileDirectory)../README.md" Pack="true" PackagePath="/" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
	</ItemGroup>

</Project>
